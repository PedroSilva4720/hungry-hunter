// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model restaurant {
  id           String @unique @default(cuid())
  name         String
  address      String
  phoneNumber  String
  email        String @unique
  passwordHash String
  createdAt    String
}
